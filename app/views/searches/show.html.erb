<h1>Search Result</h1>
<p><%= link_to 'Back', new_search_path %></p>

<% if @search.search_items.empty? %>
  <p>No Records Found</p>
<% else %>
  <% @search.search_items.each do |c| %>
    <br>
    <div class="div">
      <h1><%= c.name %></h1>
      <p><%= c.category_id %></p>
    </div>
  <% end %>
<% end %>